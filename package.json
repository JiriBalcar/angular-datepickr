{
  "name": "angular2-datepickr",
  "version": "0.3.4",
  "description": "Angular2 Datepicker",
  "main": "angular2-datepickr.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JiriBalcar/angular2-datepicker.git"
  },
  "bugs": {
    "url": "https://github.com/JiriBalcar/angular2-datepicker/issues"
  },
  "scripts": {
    "prepublish": "npm run build-css && ngc",
    "watch": "nodemon -i **/*.d.ts -w src -e scss,ts -x \"npm run prepublish\"",
    "build-css": "node-sass src -o src --output-style compressed -q"
  },
  "keywords": [
    "angular",
    "angular2",
    "datepicker",
    "date",
    "directive"
  ],
  "author": "Jiri Balcar <j.balcar.dc@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@angular/common": "^2.0.1",
    "@angular/compiler": "^2.0.1",
    "@angular/compiler-cli": "^2.1.1",
    "@angular/core": "^2.0.1",
    "@angular/forms": "^2.0.1",
    "@angular/platform-browser": "^2.0.1",
    "@types/moment": "^2.11.29",
    "@types/node": "^6.0.42",
    "moment": "^2.15.2",
    "node-sass": "^3.10.1",
    "nodemon": "^1.11.0",
    "rxjs": "5.0.0-beta.12",
    "typescript": "^2.0.3",
    "zone.js": "0.6.25"
  },
  "dependencies": {}
}
